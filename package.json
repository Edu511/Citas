{
  "name": "sirca-pgjeh",
  "version": "1.0.0",
  "description": "aplicación para guardar cosas del sirca",
  "main": "server.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/SthefanyRangelIT/SiRCA-PGJEH.git"
  },
  "scripts": {
    "client-install": "cd frontend && npm install",
    "server-install": "cd backend && npm install",
    "prod": "node dist",
    "server": "npm start --prefix backend",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "babel server -d dist",
    "test": "jest back.test.js --detectOpenHandles"
  },
  "author": "Innovación PGJEH",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.2.2"
  }
}
