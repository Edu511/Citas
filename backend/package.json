{
  "name": "sirca-pgjeh",
  "version": "1.0.0",
  "description": "aplicación para guardar cosas del sirca",
  "main": "server.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/SthefanyRangelIT/SiRCA-PGJEH.git"
  },
  "scripts": {
    "client-install": "cd client && npm install",
    "prod": "node dist",
    "server": "nodemon server/server.js --exec babel-node --trace-warnings",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "babel server -d dist",
    "test": "jest back.test.js --detectOpenHandles"
  },
