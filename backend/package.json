    "@babel/preset-env": "^7.15.0",
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@fontsource/roboto": "^4.5.0",
    "bcrypt": "^5.0.1",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.16.2",
    "express-validator": "^6.12.1",
    "jsonwebtoken": "^8.5.1",
    "logger": "0.0.1",
    "morgan": "^1.10.0",
